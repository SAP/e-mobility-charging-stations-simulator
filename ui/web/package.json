    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^21.1.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
